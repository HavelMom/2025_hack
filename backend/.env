# Port your Express server will listen on
PORT=5050

# MongoDB connection URI
# (change <your‑db‑host> as needed, or leave this default for a local MongoDB)
MONGODB_URI=mongodb://localhost:27017/healthcare-app

# Secret key for signing JWTs—replace with something strong in production
JWT_SECRET=your_jwt_secret_key_here

# Optional, controls error‐handler verbosity
NODE_ENV=development